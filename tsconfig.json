{
  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "jsx": "react",
    "module": "es2015",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noImplicitAny": false,
    "outDir": "build/",
    "paths": {
      "*": ["*", "node_modules/*"]
    },
    "target": "es5",
    "sourceMap": true
  },
  "include": ["common"]
}
